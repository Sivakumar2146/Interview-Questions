package SDTE_Program;

public class How_To_Remove_Junk_or_Special_Characters_in_String {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String s="!@#$%^&*()(*&^%$#$%^&*(*&^ Selenium";
		
	    s=	s.replaceAll("[^a-zA-Z0-9]","");
		
	    System.out.println(s);
	
		
	}

}
