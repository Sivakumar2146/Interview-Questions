package SDTE_Program;

public class Given_number_is_Prime_Even {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int num = 96;
		int count = 0;

		if (num > 1) {

			for (int i = 1; i <= num; i++) {
				if (num % i == 0)
					count++;
			}

			if (count == 2) {
				System.out.println("Is a prime number");
			}

			else {
				System.out.println("Not  a prime number");
			}

		} else {
			System.out.println("Not  a prime number");
		}

	}

}
